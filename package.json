{
  "name": "proto3d",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@craco/craco": "^6.0.0",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/tinycolor2": "^1.4.2",
    "antd": "^4.9.2",
    "craco-less": "^1.17.1",
    "drei": "^2.2.10",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.5",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.20",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rc-color-picker": "^1.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-rnd": "^10.2.4",
    "react-scripts": "4.0.1",
    "react-split-pane": "^0.1.92",
    "react-three-fiber": "^5.3.7",
    "string-replace-loader": "^2.3.0",
    "three": "^0.123.0",
    "tiny-case": "^1.0.2",
    "tinycolor2": "^1.4.2",
    "typescript": "^4.1.3",
    "valtio": "^0.4.9"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "format": "pretty-quick --staged",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src/"
          ]
        }
      }
    },
    "rules": {
      "import/order": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run format"
    }
  }
}
